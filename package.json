{
  "name": "vitals-ai",
  "version": "0.1.0",
  "private": true,
  "description": "Privacy-first AI health analytics dashboard for biomarkers, body composition, recovery, and digital twin insights.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mangeshraut712/Vitals.AI.git"
  },
  "homepage": "https://github.com/mangeshraut712/Vitals.AI",
  "bugs": {
    "url": "https://github.com/mangeshraut712/Vitals.AI/issues"
  },
  "keywords": [
    "health",
    "biomarkers",
    "digital-twin",
    "wellness",
    "nextjs",
    "react",
    "typescript",
    "privacy"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "node scripts/preflight.mjs && next dev --webpack",
    "postinstall": "prisma generate",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "node scripts/preflight.mjs --typecheck && node -e \"const fs=require('fs'); fs.rmSync('.next/types',{recursive:true,force:true}); fs.rmSync('.next/dev/types',{recursive:true,force:true});\" && next typegen && tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "doctor": "node scripts/preflight.mjs --doctor",
    "deploy:check": "node scripts/preflight.mjs --deploy-check"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.29",
    "@json-render/core": "^0.2.0",
    "@json-render/react": "^0.2.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "@types/papaparse": "^5.5.2",
    "ai": "^6.0.90",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^5.3.3",
    "framer-motion": "^12.26.2",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "pdf-parse": "^1.1.1",
    "postprocessing": "^6.38.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.182.0",
    "@vitest/ui": "^4.0.17",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
